    <display-name>empDemo</display-name>
    <servlet-name>empDemo</servlet-name>
    <servlet-class>empDemo.empDemo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>empDemo</servlet-name>
    <url-pattern>/empDemo</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>loginController</display-name>
